{
    "name": "alt3/cakephp-swagger",
    "description": "Instant Swagger documentation for your CakePHP 3.x APIs",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "~3.1",
        "zircote/swagger-php": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "support": {
        "issues": "https://github.com/alt3/cakephp-swagger/issues",
        "source": "https://github.com/alt3/cakephp-swagger"
    },
    "autoload": {
        "psr-4": {
            "Alt3\\Swagger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alt3\\Swagger\\Test\\": "tests"
        }
    }
}
